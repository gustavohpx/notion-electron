{
  "compilerOptions": {
    "strict": true,
    "target": "esnext",
    "lib": [
      "esnext",
      "dom",
      "dom.iterable"
    ],
    "types": [
      "electron-vite/node"
    ],
    "jsx": "react-jsx",
    "importHelpers": true,
    "module": "es2022",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "isolatedModules": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@*": [
        "./src/*"
      ],
    }
  },
  "include": [
    "src",
    "src/preload/index.d.ts",
    "electron.vite.config.*"
  ],
  "exclude": [
    "node_modules"
  ],
}